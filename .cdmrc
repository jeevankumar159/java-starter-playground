# .cdmrc

# Start terminal with user greeting
terminals:
  - echo "Welcome to your Java coding lab!"

# Open only Main.java file by default
tabs:
  - Main.java#5:4

# Choose layout (no browser needed for backend lab)
playground-view: terminal-editor

# Run Code button compiles + runs userâ€™s Java code
run-button: javac Main.java && java Main

# Test button runs the evaluation script
test-button: bash .codedamn/test-eval.sh
